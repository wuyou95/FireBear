<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lanou.wuyou.fearbear">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".app.BearApplication"
        android:allowBackup="true"
        android:icon="@mipmap/a"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".addoil.OilAddActivity"/>
        <activity android:name=".add.AddActivity"/>
        <activity android:name=".oilrecords.OilRecordsActivity"/>
    </application>

</manifest>